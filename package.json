{
  "name": "framer-example",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prettier:code-fix": "prettier --write",
    "lint:code-fix": "eslint --fix --ext .js,.jsx,.ts,.tsx",
    "lint:code": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:style": "stylelint **/*.(ts|tsx)",
    "postinstall": "husky install"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.23.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.5",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.7"
  },
  "lint-staged": {
    "*.json": [
      "yarn prettier:code-fix"
    ],
    ".rc": [
      "yarn prettier:code-fix"
    ],
    "*.{js,jsx,ts,tsx}": [
      "yarn lint:code-fix --cache"
    ]
  },
  "engines": {
    "node": "^18.0.0",
    "npm": "please-use-yarn",
    "yarn": "^1.22.17"
  }
}